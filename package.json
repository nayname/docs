{
  "name": "cosmos-documentation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx mintlify dev",
    "dev:global": "mintlify dev",
    "dev:dlx": "yarn dlx mintlify dev",
    "build": "npx mintlify build",
    "build:global": "mintlify build",
    "build:dlx": "yarn dlx mintlify build",
    "start": "npx mintlify start",
    "lint": "next lint",
    "broken-links": "npx mintlify broken-links",
    "broken-links:global": "mintlify broken-links",
    "install-mintlify": "npm install -g mintlify",
    "fix-sharp": "npm install --include=optional sharp",
    "clean": "rm -rf .next node_modules yarn.lock",
    "reset": "yarn clean && yarn install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tailwindcss/typography": "^0.5.10",
    "lucide-react": "^0.511.0",
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.37",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-mdx": "^3.4.2",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@4.9.0",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  }
}
