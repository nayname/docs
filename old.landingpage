---
title: "Cosmos Documentation"
description: "Unified Documentation Hub for the Cosmos Stack.."
mode: "custom"
---

import { CosmosIcon, EthereumIcon, IBCIcon, SDKIcon, APIIcon } from '/snippets/icons.mdx'

<div className="landing-page">
  <div className="landing-container">

    {/* Hero Section */}
    <div className="hero-section">
      <div className="hero-icon">
        <CosmosIcon size={64} color="#4B47CA" />
      </div>

      <h1 className="hero-title">Cosmos SDK & EVM Documentation</h1>
      <p className="hero-description">
        A hub for technical documentation, API references, and developer guides for building and interacting with Cosmos chains.
      </p>

      <div className="hero-actions">
        <a href="/docs/evm" className="btn-primary">
          <span>EVM Developer Guides</span>
          <svg className="btn-arrow" width="16" height="16" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path strokeLinecap="round" strokeLinejoin="round" strokeWidth={2} d="M13 7l5 5m0 0l-5 5m5-5H6" />
          </svg>
        </a>
        <a href="/docs/api-reference/cosmos" className="btn-secondary">
          API Reference
        </a>
      </div>
    </div>

    {/* Main Navigation Cards */}
    <div className="main-nav-section">
      <div className="nav-grid">
        <div className="nav-card primary">
          <div className="nav-icon">
            <EthereumIcon size={32} color="white" />
          </div>
          <h3>EVM on Cosmos</h3>
          <p>Technical guides for deploying Solidity contracts on a Cosmos SDK chain.</p>
          <a href="/docs/evm" className="nav-link">Explore EVM Docs →</a>
        </div>

        <div className="nav-card secondary">
          <div className="nav-icon">
            <IBCIcon size={32} color="white" />
